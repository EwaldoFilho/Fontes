function inputFields(form) {if (form && form.getValue("dataabertura") && form.getValue("dataabertura").match("^[0-3]?[0-9]/[0-3]?[0-9]/(?:[0-9]{2})?[0-9]{2}$")) {var split = form.getValue("dataabertura").split('/');form.setValue("dataabertura", split[2] + '-' + split[1] + '-' + split[0]);}if (form && form.getValue("datadespesa") && form.getValue("datadespesa").match("^[0-3]?[0-9]/[0-3]?[0-9]/(?:[0-9]{2})?[0-9]{2}$")) {var split = form.getValue("datadespesa").split('/');form.setValue("datadespesa", split[2] + '-' + split[1] + '-' + split[0]);}}