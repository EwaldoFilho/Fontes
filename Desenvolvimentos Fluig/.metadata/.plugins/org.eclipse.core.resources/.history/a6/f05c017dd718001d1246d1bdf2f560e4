function enableFields(form) {		var activity = getValue('WKNumState');if (activity == 2) {form.setEnabled('DataAbertura', false);form.setEnabled('txMaquina', false);form.setEnabled('ramal', false);form.setEnabled('cbSetor', false);form.setEnabled('txAdicoinais', false);form.setEnabled('cbcritidade', false);form.setEnabled('txSolucao', false);}if (activity == 34) {form.setEnabled('DataAbertura', false);form.setEnabled('txMaquina', false);form.setEnabled('ramal', false);form.setEnabled('cbSetor', false);form.setEnabled('txAdicoinais', false);form.setEnabled('cbcritidade', false);form.setEnabled('txValidacao', false);}if (activity == 36) {form.setEnabled('DataAbertura', false);form.setEnabled('txMaquina', false);form.setEnabled('ramal', false);form.setEnabled('cbSetor', false);form.setEnabled('txAdicoinais', false);form.setEnabled('cbcritidade', false);}if (activity == 38) {form.setEnabled('DataAbertura', false);form.setEnabled('txMaquina', false);form.setEnabled('ramal', false);form.setEnabled('cbSetor', false);form.setEnabled('txAdicoinais', false);form.setEnabled('cbcritidade', false);}if (activity == 22) {form.setEnabled('dtRetorno', false);form.setEnabled('DataAbertura', false);form.setEnabled('cbSetor', false);form.setEnabled('txAdicoinais', false);form.setEnabled('cbcritidade', false);form.setEnabled('txSolucao', false);}if (activity == 25) {form.setEnabled('dtRetorno', false);}if (activity == 13) {form.setEnabled('txSolucao', false);}}