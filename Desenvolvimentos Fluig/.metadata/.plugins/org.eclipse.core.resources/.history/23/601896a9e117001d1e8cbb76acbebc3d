SELECT 
	HISTOR_ACES.*,
	DOCUMENTO.COD_EMPRESA,
	DOCUMENTO.NR_DOCUMENTO,
	DOCUMENTO.DES_PRINC_FICHA,
	DOCUMENTO.NM_DATASET,
	DOCUMENTO.DS_COMENTARIO_ADICIONAL
	
FROM HISTOR_ACES
INNER JOIN DOCUMENTO ON DOCUMENTO.COD_EMPRESA=HISTOR_ACES.COD_EMPRESA AND DOCUMENTO.NR_DOCUMENTO=DOCUMENTO.NR_DOCUMENTO
INNER JOIN FDN_USERTENANT ON FDN_USERTENANT.LOGIN=HISTOR_ACES.CD_MATRICULA