function adicionartabledetailname11(tipodocumento) {
    var id = wdkAddChild("tabledetailname11");
    var qtd = $("#existedocumento").val();    

    $("#tipodocumento___" + id).val(tipodocumento);
    $("#informacoesadd___" + id).val();
    $("#valorDocumentos___" + id).val();

    qtd++
    $("#existedocumento").val(true);
}
