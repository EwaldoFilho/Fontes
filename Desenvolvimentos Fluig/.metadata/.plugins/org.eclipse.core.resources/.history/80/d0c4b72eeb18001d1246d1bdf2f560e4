function enableFields(form) {		
	var activity = getValue('WKNumState');
	
	if (activity != 0 && activity != 8) {
		form.setEnabled("centroCusto",false);
		form.setEnabled("NomeColaborador",false);
		form.setEnabled("cpf",false);
		form.setEnabled("email",false);
		form.setEnabled("cargos",false);
		form.setEnabled("descricaoTcontrato",false);
		form.setEnabled("Modulo",false);
		form.setEnabled("DataDesligamento",false);
		form.setEnabled("Perfil",false);
		form.setEnabled("Sistemas0",false);
		form.setEnabled("Sistemas1",false);
		form.setEnabled("Sistemas2",false);
		form.setEnabled("Sistemas4",false);
		form.setEnabled("Sistemas5",false);
		form.setEnabled("Sistemas6",false);
		form.setEnabled("Sistemas7",false);
		form.setEnabled("Sistemas8",false);
		form.setEnabled("Aviso",false);
		form.setEnabled("DiasTrabalhados",false);
		form.setEnabled("Informa_Add",false);
	}
}
