function validateForm(form) {		var activity = getValue('WKNumState');if ((form.getValue("DataAbertura") == null || form.getValue("DataAbertura") ==  "") && ( getValue('WKNumProces') == null || (getValue('WKNumProces') > 0 && getValue('WKCompletTask') == 'true') )) {throw "Data de Abertura n\u00E3o pode ser vazio.";}if ((form.getValue("txMaquina") == null || form.getValue("txMaquina") ==  "") && ( getValue('WKNumProces') == null || (getValue('WKNumProces') > 0 && getValue('WKCompletTask') == 'true') )) {throw "Identifica\u00E7\u00E3o da maquina n\u00E3o pode ser vazio.";}if ((form.getValue("cbSetor") == null || form.getValue("cbSetor") ==  "") && ( getValue('WKNumProces') == null || (getValue('WKNumProces') > 0 && getValue('WKCompletTask') == 'true') )) {throw "Setor solicitante n\u00E3o pode ser vazio.";}if ((form.getValue("txAdicoinais") == null || form.getValue("txAdicoinais") ==  "") && ( getValue('WKNumProces') == null || (getValue('WKNumProces') > 0 && getValue('WKCompletTask') == 'true') )) {throw "Informa\u00E7\u00F5es Adicionais n\u00E3o pode ser vazio.";}if ((form.getValue("cbcritidade") == null || form.getValue("cbcritidade") ==  "") && ( getValue('WKNumProces') == null || (getValue('WKNumProces') > 0 && getValue('WKCompletTask') == 'true') )) {throw "Criticidade n\u00E3o pode ser vazio.";}}