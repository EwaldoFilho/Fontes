function inputFields(form) {if (form && form.getValue("DataAbertura") && form.getValue("DataAbertura").match("^[0-3]?[0-9]/[0-3]?[0-9]/(?:[0-9]{2})?[0-9]{2}$")) {var split = form.getValue("DataAbertura").split('/');form.setValue("DataAbertura", split[2] + '-' + split[1] + '-' + split[0]);}if (form && form.getValue("dtRetorno") && form.getValue("dtRetorno").match("^[0-3]?[0-9]/[0-3]?[0-9]/(?:[0-9]{2})?[0-9]{2}$")) {var split = form.getValue("dtRetorno").split('/');form.setValue("dtRetorno", split[2] + '-' + split[1] + '-' + split[0]);}}