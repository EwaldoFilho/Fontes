function enableFields(form) {		
	var activity = getValue('WKNumState');
	
	if (activity != 0 && activity != 8) {
		form.setEnabled("codCentroCusto",false);
		form.setEnabled("centroCusto",false);
		form.setEnabled("nMaquina",false);
		form.setEnabled("telContato",false);
		form.setEnabled("Perfil",false);
		form.setEnabled("Tipo0",false);
		form.setEnabled("Tipo1",false);
		form.setEnabled("Tipo2",false);
		form.setEnabled("Tipo3",false);
		form.setEnabled("Tipo4",false);
		form.setEnabled("Tipo5",false);
		form.setEnabled("Tipo6",false);
		form.setEnabled("Tipo7",false);
		form.setEnabled("Tipo8",false);
		form.setEnabled("Tipo9",false);
		form.setEnabled("Aviso",false);
		form.setEnabled("Informa_Add",false);
	}
}
